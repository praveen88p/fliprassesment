Given a string, remove the vowels from the string.

		string str1 ;
	string removeVowels(string S) 
	{
	    // Your code goes here
	    for(int i =0 ; i<S.size(); i++){
	        if(S[i] != 'a'&& S[i] != 'e'&& S[i] != 'i'&& S[i] !='o' && S[i] !='u'){
	            str1.push_back(S[i]);
	        }
	    }
	    return str1;
	}
